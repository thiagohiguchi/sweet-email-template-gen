{# {% set backgroundColor = "#f1f1f1" %} #}
{% set maxWidth = "" %}

{% extends "../templates/base-commented.html" %}

{% block headCode %}

    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@300;600;700;800&display=swap" rel="stylesheet">

{% endblock %}

{% block inlineCss %}

    h1, h2, h3, h4, h5, h6, a, p { font-family: 'Open Sans', arial, sans-serif; } 
    
    p {
        font-size: 1.0625rem;
        font-weight: normal;
        font-stretch: normal;
        font-style: normal;
        line-height: 1.59;
        letter-spacing: 0.07px;
    }

    p a {
        font-size: 0.875rem;
        font-weight: bold;
        line-height: 2;
        letter-spacing: 0.06px;
        text-align: center;
        color: tomato;
        text-decoration: none;
    }

    h4 {
        font-size: 1.25rem;
        font-weight: bold;
        line-height: 1.3;
        letter-spacing: 0.09px;
    }

    h3 {
        font-size: 1.125rem;
        font-weight: normal;
        line-height: 1.5;
        letter-spacing: 0.08px;
    }
    
    body { background:#f1f1f1; }

    #backgroundTable { margin: 30px 0; } 

    #backgroundTd  { padding: 0 40px; } 

    .topImage{
        width: 100%;
        height: auto;
    }
    
    .center {
        margin:auto;
        text-align:center;
    }

    .backgroundWhite {
        background-color: #fff;
    }

   .backgroundGray {
        background-color: #2c3742;
    }

   .backgroundTransparent {
        background: transform !important;
    }

    .white {
        color: #fff;
    }

    .defaultTdPadding {
        padding: 40px 40px 60px;
    }

    .innerTdPadding {
        padding: 0 115px 20px;
    }

    .innerMaxWidth {
        max-width:468px;
        margin:auto;
    }

    .marginBottom10 {
        margin-bottom:10px;
    }

    .marginBottom15 {
        margin-bottom:15px;
    }

    .marginBottom40 {
        margin-bottom:40px;
    }

    .paddingBottom20 {
        padding-bottom: 20px;
    }

    .borderBottom {
        border-bottom: solid 7px #f1f1f1;
    }

    .top {
        vertical-align:top;
    }

    .listImage{
        margin: 0 11px;
    }

    .listTitle{
        font-size: 1rem;
        font-weight: bold;
        line-height: 1.75;
        letter-spacing: 0.07px;
        margin: 0 0 5px 0;
        text-align: left;
    }

    .listText{
        font-size: 0.9375rem;
        line-height: 1.6;
        letter-spacing: 0.06px;
        margin: 0;
        padding-bottom: 45px;
    }

    .fadeText {
        font-size: 0.75rem !important;
        line-height: normal;
        letter-spacing: normal;  
        color: #a1adb6;
    }

    .fadeText a{
        font-size: 0.75rem !important;
        line-height: normal;
        letter-spacing: normal;  
        font-weight: normal;
        color: #a1adb6 !important;
        text-decoration: underline !important;
    }

    .fadeTextOnTop {
        text-align: right;
        margin: 10px 0;
    }

    .button {
        border-radius: 2px;
        background-color: tomato;
        padding: 15px 25px;
        font-size: 1rem;
        font-weight: bold;
        letter-spacing: 1px;
        color: #ffffff !important;
        text-transform: uppercase !important;
    }


{% endblock %}

{% block inlineCssMaxW480 %}

    #backgroundTd { padding: 0 3% !important; } 

    p {
        font-size: 0.9825rem !important;
        line-height: 1.5 !important;
    }

    h4 {
        font-size: 1.125rem !important;
        line-height: 1.2 !important;
    }

    h3 {
        font-size: 1rem !important;
        line-height: 1.4 !important;
    }
    
    .innerTdPadding {
        padding: 0 35px 20px !important;
    }
    
    .defaultTdPadding {
        padding: 30px 20px 30px !important;
    }

    .button {
        padding: 13px 22px !important;
        font-size: 0.9375rem !important;
    }

     .listText{
         padding-bottom: 32px !important;
     }

    .listTitle{
        font-size: 0.9375rem !important;
        line-height: 1.95 !important;
    }

    .listText{
        font-size: 0.875rem !important;
        line-height: 1.85 !important;
        padding-bottom: 32px !important;
    }

    .fadeText {
        font-size: 0.625rem !important;
    }

    .fadeText a{
        font-size: 0.625rem !important;
    }


{% endblock %}

{% block inlineCssMaxW467 %}

    #backgroundTd { padding: 0 4%; } 

    .defaultTdPadding {
        padding: 25px 25px 48px;
    }
    
    .innerTdPadding {
        padding: 0 53px 20px !important;
    }

    .listTitle{
        font-size: 0.9375rem !important;
        line-height: 1.95 !important;
    }

    .listText{
        font-size: 0.875rem !important;
        line-height: 1.85 !important;
        padding-bottom: 32px !important;
    }

    .fadeText {
        font-size: 0.625rem !important;
    }

    .fadeText a{
                font-size: 0.625rem !important;
    }

{% endblock %}

